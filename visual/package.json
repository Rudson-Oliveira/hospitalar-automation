{
  "name": "hospitalar-automation-visual",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts src/**/*.js || true",
    "build": "tsc || true && cp src/web-interface/comet.html dist/comet.html && cp src/web-interface/styles.css dist/styles.css && cp src/web-interface/script.js dist/script.js && cp src/web-interface/navigate-integration.js dist/navigate-integration.js && cp -r client-build/assets dist/ 2>/dev/null || true && echo 'Build completed successfully'",
    "start": "node dist/server.js",
    "server": "ts-node src/server.ts",
    "dev:server": "ts-node src/server.ts",
    "dev": "ts-node src/web-interface/dashboard-server.ts",
    "interact": "ts-node src/interactive.ts",
    "dev:interactive": "ts-node src/interactive.ts",
    "web": "ts-node src/web-interface/server.ts",
    "sim:basic": "ts-node src/agents/simulation.ts",
    "sim:advanced": "ts-node src/agents/simulation_v2.ts",
    "sim:full": "ts-node src/agents/full_simulation.ts",
    "sim:twin": "ts-node src/agents/twin_simulation.ts",
    "sim:hierarchy": "ts-node src/agents/hierarchy_simulation.ts",
    "dashboard": "ts-node src/web-interface/dashboard-server.ts",
    "obsidian": "ts-node src/agents/obsidian-desktop.ts"
  },
  "keywords": [
    "automation",
    "hospital",
    "ai",
    "multi-agent"
  ],
  "author": "Manus AI",
  "license": "ISC",
  "description": "Sistema de Automação Hospitalar com IA Multi-Agente",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "axios": "^1.13.2",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "ghost-cursor": "^1.4.1",
    "openai": "^4.104.0",
    "playwright": "1.57.0",
    "puppeteer": "^23.10.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/uuid": "^10.0.0"
  }
